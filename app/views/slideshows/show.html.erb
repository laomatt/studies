<center>
  <h1>

  <%= @show.title %>
  </h1>
<ul class="slideshow_lists_inspect">
<% @images.each do |ss| %>
  <li>
    <img src="<%= ss.ext_url %>" alt="">
  </li>
<% end %>
</ul>
<button class="upload-button launch-study">
  Start Study
</button>
</center>
<div class="start-modal form-modal">
  <form class="launch-set" action="/slideshows/<%= @show.id %>/draw_set">
    <h2>Start study</h2>
    <input type="hidden" name="id" value="<%= @show.id %>">
    <li>How many minutes per pose?</li>
    <input type="number" value="1" name="pose_length">
    <li>How many poses per set?</li>
    <input type="number" value="20" name="pose_number">
    <li>Do you want a random order?   <input type="checkbox" name="random" value="1" checked></li>
  </form>
  <button id="start-draw-set" class="upload-button">Draw!!!</button>
</div>
